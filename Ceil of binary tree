int findCeil(BinaryTreeNode<int> *node, int x)
{
    int ceil=-1;
    if(node->data==x)
    {
        ceil=node->data;
        return ceil;
    }
   while(node)
   {
        if(x>node->data)
       {
         node=node->right;
       }
       else
      {
        ceil=node->data;
        node=node->left;
      }
   }
    return ceil;
}
